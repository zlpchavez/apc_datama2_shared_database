// SCHOOL TABLE
  CREATE TABLE "MI182_ZPCHAVEZ"."SCHOOL" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"SCH_NAME" VARCHAR2(100) NOT NULL ENABLE, 
	"SCH_ADDRESS" VARCHAR2(100) NOT NULL ENABLE, 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
  
  // CLASSES TABLE
  CREATE TABLE "MI182_ZPCHAVEZ"."CLASSES" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"CLASS_TIME" TIMESTAMP (6) WITH LOCAL TIME ZONE NOT NULL ENABLE, 
	"CLASS_SECTION" VARCHAR2(20) NOT NULL ENABLE, 
	"CLASS_ROOM" NUMBER(*,0) NOT NULL ENABLE, 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
  
  // CLINIC TABLE
    CREATE TABLE "MI182_ZPCHAVEZ"."CLINIC" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"CLIN_SCHED" TIMESTAMP (6) WITH LOCAL TIME ZONE NOT NULL ENABLE, 
	"CLIN_ASSIGN_DOC" VARCHAR2(1000) NOT NULL ENABLE, 
	"CLIN_INVENTORY" VARCHAR2(1000) NOT NULL ENABLE, 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
  
  // COURSE TABLE
    CREATE TABLE "MI182_ZPCHAVEZ"."COURSE" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"CRS_TITLE" VARCHAR2(30) NOT NULL ENABLE, 
	"CRS_DESCRIPTION" VARCHAR2(1000) NOT NULL ENABLE, 
	"CRS_UNITS" NUMBER(*,0) NOT NULL ENABLE, 
	"DEPARTMENT_ID" VARCHAR2(10) NOT NULL ENABLE, 
	"CLASSES_ID" VARCHAR2(7) NOT NULL ENABLE, 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
  
  // DEPARTMENT TABLE
  CREATE TABLE "MI182_ZPCHAVEZ"."DEPARTMENT" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"DEPT_NAME" VARCHAR2(20) NOT NULL ENABLE, 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;
  
  // STAFF TABLE
   CREATE TABLE "MI182_ZPCHAVEZ"."STAFF" 
   (	"ID" NUMBER(*,0) NOT NULL ENABLE, 
	"STAFF_FNAME" VARCHAR2(100) NOT NULL ENABLE, 
	"STAFF_MNAME" VARCHAR2(100) NOT NULL ENABLE, 
	"STAFF_LNAME" VARCHAR2(100) NOT NULL ENABLE, 
	"SCH_ID" NUMBER(*,0), 
	"STAFF_TYP" VARCHAR2(1), 
	 PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 
  TABLESPACE "USERS"  ENABLE, 
	 CONSTRAINT "STAFF_TYP" CHECK (staff_typ IN ('T', 'N')) ENABLE, 
	 FOREIGN KEY ("SCH_ID")
	  REFERENCES "MI182_ZPCHAVEZ"."SCHOOL" ("ID") ENABLE
   ) SEGMENT CREATION DEFERRED 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  TABLESPACE "USERS" ;